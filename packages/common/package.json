{
  "name": "@cube/common",
  "version": "0.1.0",
  "description": "Package of common adapters and helpers",
  "private": false,
  "files": [
    "/lib"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./adapters": {
      "require": "./lib/cjs/adapters/index.js",
      "import": "./lib/esm/adapters/index.js"
    },
    "./helpers": {
      "require": "./lib/cjs/helpers/index.js",
      "import": "./lib/esm/helpers/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf lib/",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig-build.json --outDir lib/esm",
    "build:cjs": "tsc -p tsconfig-build.json --module commonjs --outDir lib/cjs"
  },
  "author": "Carlos Souza <carlos.pessoal@hotmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "http-status": "^1.5.2"
  }
}
